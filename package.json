{
  "name": "aigen-claudecode-tetris",
  "version": "1.0.0",
  "description": "Marathon Tetris game following Tetris Design Guideline",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "profile": "source-map-explorer dist/assets/*.js"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/ui": "^1.2.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-pwa": "^0.17.4",
    "vite-plugin-singlefile": "^2.3.0",
    "vitest": "^1.2.1"
  },
  "dependencies": {
    "pixi.js": "^8.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
